{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.example.com",
    "ANALYTICS_ID": "@my-analytics-id"
  },
  
  "alias": "kiddy-learno.vercel.app",

  "build": {
    "env": {
      "SECRET_KEY": "@secret-key"
    }
  }
}
